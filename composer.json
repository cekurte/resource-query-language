{
    "name": "cekurte/rql",
    "description": "A Resource Query Language to PHP",
    "keywords": [
        "rql",
        "resource",
        "query",
        "language",
        "parser",
        "doctrine",
        "builder",
        "query string",
        "query builder",
        "filter",
        "sort"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5",
        "psr/http-message": "^1.0",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "zendframework/zend-diactoros": "^1.3",
        "phpunit/phpunit": "^4.8",
        "cekurte/tdd": "^1.0.1",
        "sjparkinson/static-review": "^5.1",
        "sensiolabs/security-checker": "^3.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "authors": [
        {
            "name": "Jo√£o Paulo Cercal",
            "email": "jpcercal@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cekurte\\Resource\\Query\\Language\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cekurte\\Resource\\Query\\Language\\Test\\": "test/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "vendor/bin/static-review.php hook:install --force bin/git/hooks/pre-commit.php .git/hooks/pre-commit"
        ]
    }
}
